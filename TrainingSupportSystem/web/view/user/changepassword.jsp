                                                        </div>

                                                        <div class="p-4">
                                                            <form action="user?action=changepass" method="Post">
                                                                <div class="row">
                                                                    <div class="col-lg-12">
                                                                        <div class="mb-3">
                                                                            <label class="form-label">Old password :</label>
                                                                            <input type="password" class="form-control" placeholder="Old password" required="" name="oldpass" value="${oldpass}">
                                                                            <p style="color: red">${mess1}</p>
                                                                        </div>
                                                                    </div><!--end col-->

                                                                    <div class="col-lg-12">
                                                                        <div class="mb-3">
                                                                            <label class="form-label">New password :</label>
                                                                            <input type="password" class="form-control" placeholder="New password" required="" name="newpass" value="${newpass}">

                                                                        </div>
                                                                    </div><!--end col-->

                                                                    <div class="col-lg-12">
                                                                        <div class="mb-3">
                                                                            <label class="form-label">Re-type New password :</label>
                                                                            <input type="password" class="form-control" placeholder="Re-type New password" required="" name="renewpass" value="${renewpass}">
                                                                            <p style="color: red">${mess2}</p>
                                                                        </div>
                                                                    </div><!--end col-->

                                                                    <div class="col-lg-12 mt-2 mb-0">
                                                                        <button class="btn btn-primary">Save password</button>
                                                                    </div><!--end col-->
                                                                </div><!--end row-->
